    "version": "0.2.0",
    "configurations": [
        {
            "name": "attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/DebugDX11/ORBIScrude.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "symbolSearchPath": "E:/symbols",
            "visualizerFile": "${workspaceRoot}/shared/isopod.natvis"
        }
    ]
}